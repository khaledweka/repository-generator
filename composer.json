{
  "name": "khaledweka/repository-generator",
  "description": "A Laravel package to generate repository pattern files based on models",
  "license": "MIT",
  "authors": [
    {
      "name": "Khaled Alwakeel",
      "email": "khaled_elwakeel@outlook.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "laravel/framework": "^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "khaledweka\\RepositoryGenerator\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "khaledweka\\RepositoryGenerator\\RepositoryGeneratorServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
