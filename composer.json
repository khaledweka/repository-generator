{
    "name": "wakeelkhaled/RepositoryGenerator",
    "description": "A Laravel package to generate repository pattern files based on models",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your@email.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "wakeelkhaled\\RepositoryGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "wakeelkhaled\\RepositoryGenerator\\RepositoryGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
